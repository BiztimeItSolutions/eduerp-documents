<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package ="com.pojo.CoursePojo">
	<class name="CoursePojo" table="course">
	
	<cache usage="read-write"/>
	
		 <id name="Course_ID">
            <generator class="assigned"/>
        </id>
       
         <property name="Course_Name"/>
          <property name="Course_Type"/>
	 	  <property name="Course_Duration"/>
	 	    <property name="Course_Fees"/>
	 	      <property name="Course_Size"/>
	 	        <property name="Deleted"/>
	 	          <property name="Effective_Date"/>
	 	            <property name="Expiry_Date"/>
	 	              <property name="Version"/>
	 	                <property name="Created_By"/>
	 	                  <property name="Created_On"/>
	 	                    <property name="Modified_By"/>
	 	                      <property name="Modified_On"/>
	 	                      
	</class>
</hibernate-mapping>