<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.biz.emp.prof.pojo.PersonalInformationsPojo" table="employee">
	
	<cache usage="read-write"/>
	
		 <id name="id">
            <generator class="identity"/>
        </id>
           
       <property name="emp_ID"/>
         <property name="first_name"/>
          <property name="middle_name"/>
	 	  <property name="last_name"/>
	 	  <property name="gender"/>
	 	    <property name="address_line1"/>
	 	      <property name="address_line2"/>
	 	        <property name="city"/>
	 	        <property name="state"/>
	 	        <property name="country"/>
	 	          <property name="pincode"/>
	 	          <property name="email_ID"/>
	 	                  <property name="phone_number"/>
	 	                  <property name="dob"/>
	 	                    <property name="blood_group"/>
	 	                      <property name="marital_status"/>
	 	                      <property name="caste"/>
	 	                      <property name="nationality"/>                    
	 	                      <property name="id_number"/>
	 	                      <property name="pan_card_number"/>
	 	                      <property name="photo"/>
	 	                      <property name="parent_emp_id"/>
	 	                      <property name="user_ID"/>
	 	                      <property name="password"/>
	 	                      
	</class>
</hibernate-mapping>